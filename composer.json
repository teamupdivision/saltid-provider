{
    "name": "teamupdivision/saltid-provider",
    "description": "SaltId auth",
    "keywords": ["oauth", "laravel"],
    "license": "MIT",
    "homepage": "https://laravel.com",
    "authors": [
        {
            "name": "Andrei Decuseara",
            "email": "andrei.decuseara@updivision.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Teamupdivision\\SaltId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teamupdivision\\SaltId\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teamupdivision\\SaltId\\SaltIdServiceProvider"
            ],
            "aliases": {
                "SaltId": "Teamupdivision\\SaltId\\Facades\\SaltId"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
